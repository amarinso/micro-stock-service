{
  "services": {
    "name": "stock",
    "version": "v1"
  },
  "models": {
    "stockModel": "./models/stockModel",
    "reserveModel": "./models/reserveModel"
  },
  "hooks": {
    "preReserveStock": {
      "handler": "./operations/reserve/hooks/preReserveStock"
    },
    "reserveStock": {
      "active": true,
      "handler": "./operations/reserve/hooks/reserveStock",
      "minutesToReserve": 2,
      "allowReserveTimeOverwrite": false
    },
    "unreserveStock": {
      "active": true,
      "handler": "./operations/reserve/hooks/unreserveStock"
    }
  },
  "defaultwarehouseId": "001",
  "workers": [
    {
      "worker": "unreserveStock",
      "queue": "unreserveStock",
      "when": [
        "0 */1 * * * *"
      ]
    }
  ]
}
