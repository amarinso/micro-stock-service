{
  "services": {
    "name": "stock",
    "version": "v1"
  },
  "models": {
    "stockModel": "./models/stockModel",
    "reserveModel": "./models/reserveModel"
  },
  "hooks": {
    "preReserveStock": {
      "handler": "./operations/reserve/hooks/preReserveStock"
    },
    "reserveStock": {
      "active": true,
      "handler": "./operations/reserve/hooks/reserveStock",
      "minutesToReserve": 2,
      "allowReserveTimeOverwrite": false
    },
    "postReserveStock": {
      "handler": "./operations/reserve/hooks/postReserveStock"
    },
    "preUnreserveStock": {
      "handler": "./operations/unreserve/hooks/preUnreserveStock"
    },
    "unreserveStock": {
      "handler": "./operations/unreserve/hooks/unreserveStock"
    },
    "postUnreserveStock": {
      "handler": "./operations/unreserve/hooks/postUnreserveStock"
    }
  },
  "defaultwarehouseId": "001",
  "workers": [
    {
      "worker": "unreserveExpired",
      "handler": "./jobs/unreserveExpired",
      "when": "0 */1 * * * *"
    }
  ]
}
