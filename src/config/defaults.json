{
  "services": {
    "name": "stock",
    "version": "v1"
  },
  "models": {
    "stockModel": "./models/stockModel",
    "reserveModel": "./models/reserveModel"
  },
  "hooks": {
    "preReserveStock": {
      "handler": "./operations/reserve/hooks/preReserveStock"
    },
    "reserveStock": {
      "active": true,
      "handler": "./operations/reserve/hooks/reserveStock",
      "minutesToReserve": 2,
      "allowReserveTimeOverwrite": false
    "postReserveStock": {
      "handler": "./operations/reserve/hooks/postReserveStock"
    },
    }
  },
  "defaultwarehouseId": "001",
  "workers": [
    {
      "worker": "unreserveExpired",
      "queue": "unreserveExpired",
      "when": [
        "0 */1 * * * *"
      ],
      "handler": "./jobs/unreserveExpired"
    }
  ]
}
